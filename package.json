{
  "name": "typething",
  "version": "1.0.0",
  "description": "A project to demonstrate Google Geocoding with TypeScript",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "generate-graph": "ts-node src/generarGrafo.ts --estado durango --estado nayarit --estado sinaloa",
    "map-load": "ts-node src/visualizador.ts",
    "route-load": "ts-node src/visualizadorRuta.ts",
    "route-search": "ts-node src/buscarRuta --origen 25 --destino 88"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.1",
    "cli-progress": "^3.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "inquirer": "^12.6.3",
    "minimist": "^1.2.8",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.10.2",
    "undici": "^7.11.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.19.1",
    "@types/puppeteer": "^5.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
